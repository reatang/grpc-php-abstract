{
    "name": "reatang/grpc-php-abstract",
    "description": "php兼容 grpc原生客户端 和 grpc-gateway 调用",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "唐小鸭",
            "email": "tangtang1251@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Reatang\\GrpcPHPAbstract\\": "src/",
            "Google\\Api\\": "pb/Google/Api/",
            "GPBMetadata\\Google\\Api\\": "pb/GPBMetadata/Google/Api/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reatang\\GrpcPHPAbstract\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "grpc/grpc": "1.52.*",
        "google/protobuf": "v3.21.12",
        "ext-protobuf": "*",
        "ext-grpc": "*",
        "guzzlehttp/guzzle": "^7.5",
        "psr/log": "^1.0.1",
        "open-telemetry/api": "^1.0",
        "open-telemetry/context": "^1.0",
        "open-telemetry/sdk": "^1.0",
        "open-telemetry/sem-conv": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
